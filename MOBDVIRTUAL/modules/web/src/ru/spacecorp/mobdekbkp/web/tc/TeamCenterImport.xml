<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="ru.spacecorp.mobdekbkp.web.tc.Teamcenterimport"
        messagesPack="ru.spacecorp.mobdekbkp.web.tc">
    <dialogMode height="600"
                width="800"/>
    <layout expand="span">
        <buttonsPanel margin="false,false,true,false">
            <button id="createCountry"
                    caption="msg://createCountry"
                    invoke="onCreateCountryClick"/>
            <button id="createCompany"
                    caption="msg://createCompany"
                    invoke="onCreateCompanyClick"/>
            <button id="createCompanyHoldDocs"
                    caption="msg://createCompanyHoldDocs"
                    invoke="onCreateCompanyHoldDocsClick"/>
        </buttonsPanel>
        <buttonsPanel margin="true,false,false,false">
            <button id="start"
                    caption="msg://StartImport"
                    invoke="onStartClick"/>
        </buttonsPanel>
        <label id="span"/>
    </layout>
</window>
