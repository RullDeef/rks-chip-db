<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="ru.spacecorp.mobdekbkp.web.saprdata.SaprDataEdit"
        datasource="saprDataDs"
        focusComponent="fieldGroup"
        messagesPack="ru.spacecorp.mobdekbkp.web.saprdata">
    <dsContext>
        <datasource id="saprDataDs"
                    class="ru.spacecorp.mobdekbkp.entity.SaprData"
                    view="saprData-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="saprDataDs">
            <column width="250px">
                <field property="libraryGraphicalSymbols"/>
                <field property="electronicGeometricLibrary"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
