<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://browseCaption"
        class="ru.spacecorp.mobdekbkp.web.customparameters.CustomParametersBrowse"
        focusComponent="customParametersesTable"
        lookupComponent="customParametersesTable"
        messagesPack="ru.spacecorp.mobdekbkp.web.typonominal">
    <dsContext>
        <groupDatasource id="customParametersDs"
                         class="ru.spacecorp.mobdekbkp.entity.CustomParameters"
                         view="customParameters-view">
            <query>
                <![CDATA[select e from mobdekbkp$CustomParameters e]]>
            </query>
        </groupDatasource>
        <groupDatasource id="mainParametersesDs"
                         class="ru.spacecorp.mobdekbkp.entity.MainParameters"
                         view="mainParameters-view">
            <query>
                <![CDATA[select e from mobdekbkp$MainParameters e]]>
            </query>
        </groupDatasource>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout spacing="true">
        <scrollBox height="100%">
            <groupBox spacing="true">
                <hbox spacing="true"
                      width="100%">
                    <table id="mainParametersesTable"
                           width="100%">
                        <columns>
                            <column id="tPlus"/>
                            <column id="tMinus"/>
                            <column id="radiationResistance"/>
                            <column id="distinctiveSign"/>
                            <column id="operatingFrequencyaRange"/>
                            <column id="adjustmentFactor"/>
                            <column id="phaseNoiseLevel"/>
                            <column id="outputPower"/>
                            <column id="supplyVoltage"/>
                            <column id="consumptionCurrent"/>
                            <column id="technology"/>
                            <column id="memoryCapacity"/>
                            <column id="numberValves"/>
                            <column id="builtMemory"/>
                            <column id="maximumAllowableReverseVoltage"/>
                            <column id="maximumPermissibleAverageForwardCurrent"/>
                            <column id="maximumAllowabledirectCurrent"/>
                            <column id="limitingFrequency"/>
                            <column id="reverseRecoveryTime"/>
                            <column id="directForwardCurrent"/>
                            <column id="constantForwardVoltage"/>
                            <column id="radiationPower"/>
                            <column id="wavelength"/>
                            <column id="fallTime"/>
                            <column id="averagePowerLaseRadiation"/>
                            <column id="laserWaveLength"/>
                            <column id="divergenceLaserRadiation"/>
                            <column id="pulseRepetitionFrequencyLaserRadiation"/>
                            <column id="outputPower2"/>
                            <column id="workingFrequency"/>
                            <column id="powerDissipatedAnode"/>
                            <column id="screenBrightness"/>
                            <column id="resolution"/>
                            <column id="modulationVoltage"/>
                            <column id="spectralSensitivityRegion"/>
                            <column id="numberPhotosensitiveElements"/>
                            <column id="geometricDimensionsPhotosensitiveElement"/>
                            <column id="specificThresholdFlux"/>
                            <column id="currentIntegralSensitivity"/>
                            <column id="glowBrightness"/>
                            <column id="glowColor"/>
                            <column id="maximumPermissibleExternalIllumination"/>
                            <column id="frequencyRange"/>
                            <column id="tuningAccuracy"/>
                            <column id="inRangeOperatingTemperatures"/>
                            <column id="resonatorHousingSymbol"/>
                            <column id="ratedPowerDissipation"/>
                            <column id="nominalResistance"/>
                            <column id="resistanceTolerance"/>
                            <column id="limitingOperatingCurrent"/>
                            <column id="ratedVoltage"/>
                            <column id="ratedCapacity"/>
                            <column id="dimensions"/>
                            <column id="capacityTolerance"/>
                            <column id="impactShearForce"/>
                            <column id="minimumDutyCycle"/>
                            <column id="pulseDuration"/>
                            <column id="switchedCurrent"/>
                            <column id="switchingVoltage"/>
                            <column id="numberContactGroups"/>
                            <column id="switchingCurrentFrequency"/>
                            <column id="attachedCableBrand"/>
                            <column id="structuralPerformance"/>
                            <column id="pathType"/>
                            <column id="power"/>
                            <column id="rotationFrequency"/>
                            <column id="frequencyRange2"/>
                            <column id="nominalVoltage"/>
                            <column id="ratedCapacity2"/>
                            <column id="overallDimensions"/>
                            <column id="testVoltage"/>
                            <column id="attenuationCoefficient"/>
                            <column id="iputVoltage"/>
                            <column id="outputVoltage"/>
                            <column id="outputCurrentChannel"/>
                            <column id="diameterPitchNut"/>
                            <column id="insertedOpticalLoss"/>
                            <column id="numberArticulationsDismemberments"/>
                            <column id="numberOpticalPoles"/>
                            <column id="voltage"/>
                            <column id="electricCurrent"/>
                            <column id="luminousFlux"/>
                            <column id="minimumOperatingTime"/>
                            <column id="initialRelativeMagneticPermeability"/>
                            <column id="relativeGoodness"/>
                            <column id="qfactorMeasurementFrequency"/>
                            <column id="coefficientAdjustmentArmorCores"/>
                        </columns>
                        <rows datasource="mainParametersesDs"/>
                    </table>
                </hbox>
                <hbox spacing="true"
                      width="100%">
                    <groupTable id="customParametersesTable"
                                width="100%">
                        <actions>
                            <action id="create"
                                    invoke="onCreateParamClick"/>
                            <action id="edit"/>
                            <action id="remove"/>
                        </actions>
                        <columns>
                            <column id="name"/>
                            <column id="paramValue"/>
                        </columns>
                        <rows datasource="customParametersDs"/>
                        <rowsCount/>
                        <buttonsPanel id="buttonsPanel"
                                      alwaysVisible="true">
                            <button id="createBtn"
                                    action="customParametersesTable.create"
                                    caption="msg://Create"/>
                            <button id="editBtn"
                                    action="customParametersesTable.edit"/>
                            <button id="removeBtn"
                                    action="customParametersesTable.remove"/>
                        </buttonsPanel>
                    </groupTable>
                </hbox>
            </groupBox>
        </scrollBox>
    </layout>
</window>
