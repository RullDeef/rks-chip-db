<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="ru.spacecorp.mobdekbkp.web.typonominal.frames.Tninstallparams"
        messagesPack="ru.spacecorp.mobdekbkp.web.typonominal">
    <layout>
        <scrollBox id="scrollBox"
                   height="100%">
            <tabSheet align="TOP_CENTER">
                <tab id="tabManual"
                     caption="msg://Manual"
                     margin="true,false,false,false"
                     spacing="true">
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxTemperatureWP"
                               width="100%"/>
                        <textField id="tfMaxTemperatureWP"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxSolderingTimeOutputМакс"
                               width="100%"/>
                        <textField id="tfMaxSolderingTimeOutput"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxCaseTemperature"
                               width="100%"/>
                        <textField id="tfMaxCaseTemperatureManual"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MinDistanceFromHousingToSoldering"
                               width="100%"/>
                        <textField id="tfMinDistanceFromHousingToSoldering"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://Flux"
                               width="100%"/>
                        <textField id="tfFlux"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://Solder"
                               width="100%"/>
                        <textField id="tfSolder"
                                   width="100%"/>
                    </hbox>
                </tab>
                <tab id="tabAutomated"
                     caption="msg://Automated"
                     margin="true,false,false,false"
                     spacing="true">
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxPreHeatingTemperature"
                               width="100%"/>
                        <textField id="tfMaxPreHeatingTemperature"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxPreheatingTime"
                               width="100%"/>
                        <textField id="tfMaxPreheatingTime"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxTemperatureSoldering"
                               width="100%"/>
                        <textField id="tfMaxTemperatureSoldering"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxSolderingTime"
                               width="100%"/>
                        <textField id="tfMaxSolderingTime"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://MaxCaseTemperature"
                               width="100%"/>
                        <textField id="tfMaxCaseTemperatureAuto"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://ThermalProfile"
                               width="100%"/>
                        <textField id="tfThermalProfile"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://SolderingPasteFlux"
                               width="100%"/>
                        <textField id="tfSolderingPasteFlux"
                                   width="100%"/>
                    </hbox>
                    <hbox spacing="true"
                          width="100%">
                        <label value="msg://Method"
                               width="100%"/>
                        <textField id="tfMethod"
                                   width="100%"/>
                    </hbox>
                </tab>
            </tabSheet>
        </scrollBox>
    </layout>
</window>
