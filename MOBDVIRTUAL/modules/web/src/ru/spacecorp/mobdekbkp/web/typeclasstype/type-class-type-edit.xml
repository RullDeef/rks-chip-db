<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="ru.spacecorp.mobdekbkp.web.typeclasstype.TypeClassTypeEdit"
        datasource="typeClassTypeDs"
        focusComponent="fieldGroup"
        messagesPack="ru.spacecorp.mobdekbkp.web.typeclasstype">
    <dsContext>
        <datasource id="typeClassTypeDs"
                    class="ru.spacecorp.mobdekbkp.entity.TypeClassType"
                    view="typeClassType-view"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="130"
                width="400"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="typeClassTypeDs">
            <column width="250px">
                <field property="name"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
