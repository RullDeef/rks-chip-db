<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="ru.spacecorp.mobdekbkp.web.typonominal.frames.Tnbody"
        messagesPack="ru.spacecorp.mobdekbkp.web.typonominal">
    <dsContext>
        <datasource id="corpusDs"
                    class="ru.spacecorp.mobdekbkp.entity.Corpus"
                    view="corpus-view"/>
    </dsContext>
    <layout>
        <scrollBox height="100%">
            <groupBox spacing="true">
                <hbox spacing="true"
                      width="100%">
                    <label value="msg://corpusType"
                           width="100%"/>
                    <textField id="tfcorpusType"
                               width="100%"/>
                </hbox>
                <hbox spacing="true"
                      width="100%">
                    <label value="msg://corpusMaterial"
                           width="100%"/>
                    <textField id="tfcorpusMaterial"
                               width="100%"/>
                </hbox>
                <hbox spacing="true"
                      width="100%">
                    <label value="msg://coverCover"
                           width="100%"/>
                    <textField id="tfcoverCover"
                               width="100%"/>
                </hbox>
                <hbox spacing="true"
                      width="100%">
                    <label value="msg://contactType"
                           width="100%"/>
                    <textField id="tfcontactType"
                               width="100%"/>
                </hbox>
                <hbox spacing="true"
                      width="100%">
                    <label value="msg://contactCover"
                           width="100%"/>
                    <textField id="tfcontactCover"
                               width="100%"/>
                </hbox>
                <hbox spacing="true"
                      width="100%">
                    <label value="msg://designationOutputFormingOption"
                           width="100%"/>
                    <textField id="tfdesignationOutputFormingOption"
                               width="100%"/>
                </hbox>
            </groupBox>
        </scrollBox>
    </layout>
</window>
