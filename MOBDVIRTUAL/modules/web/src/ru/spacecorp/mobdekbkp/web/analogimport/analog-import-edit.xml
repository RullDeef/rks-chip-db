<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="ru.spacecorp.mobdekbkp.web.analogimport.AnalogImportEdit"
        datasource="analogImportDs"
        focusComponent="fieldGroup"
        messagesPack="ru.spacecorp.mobdekbkp.web.analogimport">
    <dsContext>
        <datasource id="analogImportDs"
                    class="ru.spacecorp.mobdekbkp.entity.AnalogImport"
                    view="analogImport-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="analogImportDs">
            <column width="250px">
                <field property="typonominalAnalogType"/>
                <field property="isRecommendedGnio"/>
                <field property="analogName"/>
                <field property="producer"/>
                <field property="producingCountry"/>
                <field property="okrMinPromTorg"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
