<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="ru.spacecorp.mobdekbkp.web.customparameters.CustomParametersEdit"
        datasource="customParametersDs"
        focusComponent="fieldGroup"
        messagesPack="ru.spacecorp.mobdekbkp.web.customparameters">
    <dsContext>
        <datasource id="customParametersDs"
                    class="ru.spacecorp.mobdekbkp.entity.CustomParameters"
                    view="customParameters-view"/>
    </dsContext>
    <dialogMode height="200"
                width="500"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    align="TOP_CENTER"
                    datasource="customParametersDs">
            <column width="250px">
                <field property="name"/>
                <field property="paramValue"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               align="MIDDLE_CENTER"
               screen="editWindowActions"
               width="AUTO"/>
    </layout>
</window>
