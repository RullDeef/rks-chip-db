<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://LabelAddName"
        class="ru.spacecorp.mobdekbkp.web.devicelistproject.Faddname"
        messagesPack="ru.spacecorp.mobdekbkp.web.devicelistproject">
    <dialogMode height="200"
                width="400"/>
    <layout>
        <vbox expand="textField"
              height="100%"
              spacing="true">
            <label id="labelName"
                   value="msg://LabelAddName"
                   width="100%"/>
            <textField id="textField"
                       width="100%"/>
            <hbox spacing="true"
                  width="100%">
                <button id="btOk"
                        caption="msg://btOk"
                        invoke="onBtOkClick"
                        width="100%"/>
                <button id="btCancel"
                        caption="msg://btCancel"
                        invoke="onBtCancelClick"
                        width="100%"/>
            </hbox>
        </vbox>
    </layout>
</window>
