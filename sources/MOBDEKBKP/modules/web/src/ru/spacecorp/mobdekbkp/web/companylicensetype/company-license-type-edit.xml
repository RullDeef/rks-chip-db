<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="ru.spacecorp.mobdekbkp.web.companylicensetype.CompanyLicenseTypeEdit"
        datasource="companyLicenseTypeDs"
        focusComponent="fieldGroup"
        messagesPack="ru.spacecorp.mobdekbkp.web.companylicensetype">
    <dsContext>
        <datasource id="companyLicenseTypeDs"
                    class="ru.spacecorp.mobdekbkp.entity.CompanyLicenseType"
                    view="companyLicenseType-view"/>
    </dsContext>
    <dialogMode forceDialog="true"
                height="330"
                width="450"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="companyLicenseTypeDs">
            <column width="300px">
                <field property="name"
                       rows="10"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
