                <label id="lblHasSubstitute"
                       align="MIDDLE_LEFT"
                       visible="false"
                       width="100%"/>
                <label id="lblSealNeeded"
                       align="MIDDLE_LEFT"
                       visible="false"
                       width="100%"/>
                <label id="lblIsAutoAssemble"
                       align="MIDDLE_LEFT"
                       visible="false"
                       width="100%"/>
            </vbox>
        </scrollBox>
    </layout>
</window>
